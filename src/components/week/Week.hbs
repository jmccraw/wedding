{{#each week.weeks}}
<article class="fpi-week is-{{@index}}{{#if isSuperbowl}} is-superbowl{{/if}}">
  {{#if this.weeks}}
  {{> ./header/Header}}
  {{/if}}
  {{#if this.openerImage}}
  {{> ./opener/Opener}}
  {{/if}}
  {{#if this.introduction}}
  {{> ./introduction/Introduction}}
  {{/if}}
  {{#if this.weeklyWinners}}
  {{> ./weekly-winners/WeeklyWinners}}
  {{/if}}
  {{#if this.analysis}}
  {{> ./analysis/Analysis}}
  {{/if}}
  {{#if this.performers}}
  {{> ./performers/Performers}}
  {{/if}}
  {{#if this.tables}}
  {{> ./table/Table}}
  {{/if}}
</article>
{{/each}}